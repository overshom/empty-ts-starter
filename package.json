{
  "name": "overshom-typescript",
  "version": "1.0.0",
  "author": "Oleksii Shnyra",
  "description": "TypeScript quick starter by overshom",
  "keywords": [
    "TypeScript",
    "starter",
    "overshom"
  ],
  "license": "ISC",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --exec 'ts-node' src -e ts",
    "start:prod": "node .",
    "prebuild:dev": "rimraf dist",
    "build:dev": "tsc --p tsconfig.json",
    "prebuild:prod": "rimraf dist",
    "build:prod": "tsc --p tsconfig.json"
  },
  "dependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "axios": "1.6.2",
    "cors": "2.8.5",
    "express": "4.18.2",
    "nodemon": "3.0.2"
  },
  "devDependencies": {
    "@types/node": "16.4.10",
    "rimraf": "3.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}
